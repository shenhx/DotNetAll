    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />

  </configSections>
  <connectionStrings>
    <add name="DbConn" connectionString="Data Source=D:\Projects\DotNet\Demos\DotNetAll\SqliteApplication\SqliteApplication\bin\Debug\test.db3;Initial Catalog=test.db3;Pooling=true;FailIfMissing=false"
