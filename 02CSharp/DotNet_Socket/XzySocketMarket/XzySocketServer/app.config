<?xml version="1.0"?>
<configuration>

  <configSections>
    <section name="socketServer"
             type="XzySocketServer.Config.SocketServerConfig, XzySocketServer"/>
  </configSections>

  <socketServer>
    <servers>
      <server name="demo1"
              port="9999"
              socketBufferSize="8192"
              messageBufferSize="8192"
              maxMessageSize="102400"
              maxConnections="20000"
              serviceType="service type"
              protocol="thrift|commandLine|custom protocol type"/>
    </servers>
  </socketServer>

</configuration>